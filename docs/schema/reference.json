{
    "subj": {
        "description": "Subject id (e.g., TD17)",
        "type": "string"
    },
    "month": {
        "description": "Recording month (e.g., M3)",
        "type": "string"
    },
    "act": {
        "description": "Activity code (e.g., A2, A4)",
        "type": "string"
    },
    "prefix": {
        "description": "Expected file prefix in format: TD##-M#_A#",
        "type": "string",
        "example": "TD17-M3_A2"
    },
    "path": {
        "description": "Relative path to the directory containing .txt file",
        "type": "string",
        "example": "TD17/TD17_M3"
    }
}
